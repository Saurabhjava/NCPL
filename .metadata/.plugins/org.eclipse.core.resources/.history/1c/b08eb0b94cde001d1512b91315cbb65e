package com;
class Animal {
	public void eat() {
		System.out.println("Animal Eat");
	}
}
class Dog extends Animal {
	@Override
	public void eat() {
		System.out.println("Dog Eat");
	}
}
public class TestAnimal {

	public static void main(String[] args) {
		Animal obj=new Dog();
		//Dog obj=new Animal(); // Compilation Error
		obj.eat();

	}

}

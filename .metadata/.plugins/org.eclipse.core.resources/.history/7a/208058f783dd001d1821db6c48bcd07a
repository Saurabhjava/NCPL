package com;

abstract class Car {
	public abstract void changeGear(); 
	public abstract void lockingSystem(); 
}

class Honda extends Car {
	
}
class Santro extends Car {
	
}
public class TestCar {
	public static void main(String[] args) {
		Honda h=new Honda();
		h.changeGear();
		h.lockingSystem();
		System.out.println("===========================");
		Santro s=new Santro();
		s.changeGear();
		s.lockingSystem();
	}
}
